const myFunc = async () => {
    const userRes = await fetch('https://jsonplaceholder.typicode.com/users');
    const users = await userRes.json();
    const user = users[1];
    console.log(user);
    const posResp = await fetch('https://jsonplaceholder.typicode.com/posts?userId=' + user.id);
    const posts = await posResp.json();
    console.log(posts);
}